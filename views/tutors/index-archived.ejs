<div class="jumbotron" style="background-color:#d9edf7">
  <div class="container">
    <div class="row">
      <div class="col-sm-6 offset-sm-3">
        <h2 class="display-4">tutors</h2>
        <hr class="my-4">
        <p>archived</p>
      </div>
    </div>
  </div>
</div>
<div class="container my-5">
  <div class="row">
    <div class="col-sm-6 offset-sm-3">
      <div class="d-flex flex-column flex-md-row">
        <a href="/tutors/" class="btn btn-primary my-2 mx-2">Active tutors</a>
      </div>
    </div>
  </div>
</div>
<div class="container mb-5">
  <div class="row">
    <div class="col-sm-6 offset-sm-3">
      <% if (allTutors.length > 0) { %>
        <div class="list-group">
          <% allTutors.forEach(function (tutor) { %>
            <a class="list-group-item list-group-item-action" href="/tutors/<%= tutor.id %>">
              <%= tutor.name %>
            </a>
          <% }) %>
        </div>
      <% } else { %>
        No archived tutors yet
      <% } %>
    </div>
  </div>
</div>
